<!-- unece.code.list
Source : http://www.unece.org/trade/untdid/d97b/uncl/uncl5189.htm  -->
    <record id="allowance_1" model="unece.code.list">
        <field name="type">allowance</field>
        <field name="code">1</field>
        <field name="name">Handling commission</field>
        <field name="description">Fee for the processing of documentary credit,
collection and payment which are charged to the customer.
        </field>
    </record>
    <record id="allowance_2" model="unece.code.list">
        <field name="type">allowance</field>
        <field name="code">2</field>
        <field name="name">Amendment commission</field>
        <field name="description">Fee for amendments in documentary credit and collection
business (not extensions and increases of documentary
credits).
        </field>
    </record>
    <record id="allowance_3" model="unece.code.list">
        <field name="type">allowance</field>
        <field name="code">3</field>
        <field name="name">Acceptance commission</field>
        <field name="description">Fee for the acceptance of draft in documentary credit and
collection business which are drawn on us (also to be
seen as a kind of 'guarantee commission').
        </field>
    </record>
    <record id="allowance_4" model="unece.code.list">
        <field name="type">allowance</field>
        <field name="code">4</field>
        <field name="name">Commission for obtaining acceptance</field>
        <field name="description">Fee for obtaining an acceptance under collections on the
basis of 'documents against acceptance'.
        </field>
    </record>
    <record id="allowance_5" model="unece.code.list">
        <field name="type">allowance</field>
        <field name="code">5</field>
        <field name="name">Commission on delivery</field>
        <field name="description">Fee for delivery of documents without corresponding payment.
        </field>
    </record>
    <record id="allowance_6" model="unece.code.list">
        <field name="type">allowance</field>
        <field name="code">6</field>
        <field name="name">Advising commission</field>
        <field name="description">Fee for advising documentary credits (charged also in
case of confirmed credits).
        </field>
    </record>
    <record id="allowance_7" model="unece.code.list">
        <field name="type">allowance</field>
        <field name="code">7</field>
        <field name="name">Confirmation commission</field>
        <field name="description">Fee for confirmation of credit.
        </field>
    </record>
    <record id="allowance_8" model="unece.code.list">
        <field name="type">allowance</field>
        <field name="code">8</field>
        <field name="name">Deferred payment commission</field>
        <field name="description">Fee for the deferred payment period under documentary
credits confirmed by bank. This fee are charges for the
period from presentation of the document until due date
of payment.
        </field>
    </record>
    <record id="allowance_9" model="unece.code.list">
        <field name="type">allowance</field>
        <field name="code">9</field>
        <field name="name">Commission for taking up documents</field>
        <field name="description">Fee charged to the foreign bank for the processing of
documentary credit.
        </field>
    </record>
    <record id="allowance_10" model="unece.code.list">
        <field name="type">allowance</field>
        <field name="code">10</field>
        <field name="name">Opening commission</field>
        <field name="description">Fee for opening revocable documentary credit.
        </field>
    </record>
    <record id="allowance_11" model="unece.code.list">
        <field name="type">allowance</field>
        <field name="code">11</field>
        <field name="name">Fee for payment under reserve</field>
        <field name="description">Fee charged to the customer for discrepancies in credit
documents in the case of which the bank have to stipulate
payment under reserve.
        </field>
    </record>
    <record id="allowance_12" model="unece.code.list">
        <field name="type">allowance</field>
        <field name="code">12</field>
        <field name="name">Discrepancy fee</field>
        <field name="description">Fee charged to the foreign bank for discrepancies in
credit documents.
        </field>
    </record>
    <record id="allowance_13" model="unece.code.list">
        <field name="type">allowance</field>
        <field name="code">13</field>
        <field name="name">Domicilation commission</field>
        <field name="description">Fee for the domicilation of bills with the bank.
        </field>
    </record>
    <record id="allowance_14" model="unece.code.list">
        <field name="type">allowance</field>
        <field name="code">14</field>
        <field name="name">Commission for release of goods</field>
        <field name="description">Commission for the release of goods sent to the bank.
        </field>
    </record>
    <record id="allowance_15" model="unece.code.list">
        <field name="type">allowance</field>
        <field name="code">15</field>
        <field name="name">Collection commission</field>
        <field name="description">Fee for settling collections on the basis of 'documents  against payments'.
        </field>
    </record>
    <record id="allowance_16" model="unece.code.list">
        <field name="type">allowance</field>
        <field name="code">16</field>
